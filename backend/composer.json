{
  "name": "progi/bid-calculator",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.0",
    "symfony/runtime": "^6.0",
    "symfony/flex": "^2.0",
    "symfony/yaml": "^6.0",
    "nelmio/cors-bundle": "^2.5",
    "symfony/console": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^6.0",
    "symfony/css-selector": "^6.0",
    "symfony/phpunit-bridge": "^7.2",
    "symfony/dotenv": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Progi\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
